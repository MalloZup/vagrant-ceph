#!/opt/salt2015/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'salt==2015.8.1','console_scripts','salt-api'
__requires__ = 'salt==2015.8.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('salt==2015.8.1', 'console_scripts', 'salt-api')()
    )
